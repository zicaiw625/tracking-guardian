{
  "name": "tracking-guardian",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "remix vite:build",
    "dev": "shopify app dev",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "shopify": "shopify",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "generate": "prisma generate",
    "setup": "prisma generate && prisma migrate deploy",
    "docker-start": "npm run setup && npm run start",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "render-build": "npm install && npx prisma generate && npx prisma db push && npm run build"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@remix-run/dev": "^2.15.0",
    "@remix-run/node": "^2.15.0",
    "@remix-run/react": "^2.15.0",
    "@remix-run/serve": "^2.15.0",
    "@shopify/app-bridge-react": "^4.1.2",
    "@shopify/polaris": "^13.0.0",
    "@shopify/shopify-api": "^11.0.0",
    "@shopify/shopify-app-remix": "^3.3.0",
    "@shopify/shopify-app-session-storage-prisma": "^5.2.0",
    "chart.js": "^4.4.1",
    "isbot": "^5.1.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "resend": "^3.2.0",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@shopify/api-codegen-preset": "^1.0.0",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "typescript": "^5.4.2"
  },
  "overrides": {
    "@shopify/shopify-app-remix": {
      "@remix-run/server-runtime": "$@remix-run/server-runtime"
    }
  }
}

